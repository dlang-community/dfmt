{
    "name": "dfmt",
    "description": "Dfmt is a formatter for D source code",
    "targetType": "autodetect",
    "license": "BSL-1.0",
    "dependencies": {
        "dmd": "~>2.107.0-beta.1"
    },
    "targetPath": "bin/",
    "targetName": "dfmt",
    "stringImportPaths": [
        "bin"
    ],
    "versions": [
        "built_with_dub"
    ],
    "preBuildCommands": [
        "$DC -run \"$PACKAGE_DIR/dubhash.d\""
    ]
}
